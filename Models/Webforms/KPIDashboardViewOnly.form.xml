ll().Select(a=> a.Id);\n\t\tBUIds.AddRange(getBusinessUnitsIds);\n\t}\n\t\n\tif (catIdJoined == \"0\") {\n\t\tvar getCategoryIds = Domain.Category.GetAll().Select(a=> a.Id);\n\t\tcategoryIds.AddRange(getCategoryIds);\n\t}\n\tif (subcatIdJoined == \"0\") {\n\t\tvar getSubCatIds = Domain.SubCategory.GetAll().Select(a=> a.Id);\n\t\tSubcategoryIds.AddRange(getSubCatIds);\n\t}\n\tif (baseUnitIdJoined == \"0\") {\n\t\tvar getBaseUnitsIds = Domain.BaseUnit.GetAll().Select(a=> a.Id);\n\t\tbaseUnitIds.AddRange(getBaseUnitsIds);\n\t}\n\t\n\tif (skusJoined == \"0\") {\n\t\tModel.SelectedItemSKUs = Domain.Item.GetAll().Select(a=> a.SKU);\n\t}\n\t\n\tDebugLib.Logger.WriteInfoLine(\"** SupIds: \" + supIds.Length);\n\t\n\tModel.SelectedSuppliers = supIds;\n\tModel.SelectedWarehouses = warehouseIds;\n\tModel.SelectedBUs = BUIds;\n\tModel.SelectedCategories = categoryIds;\n\tModel.SelectedSubcategories = SubcategoryIds;\n\tModel.SelectedBaseUnits = baseUnitIds;\n\t\n\t\n\t\n\t\n}","Name":"ViewFiltered","Permissions":["IsStockManager"],"AllowAnonymous":false,"AllowAllAuthenticatedUsers":false,"AccessLog":true,"IsEntryPoint":true,"IsDefault":false,"CausesValidation":false,"DontPostViewModel":false}],"EventListeners":[],"Resources":[{"Type":"S","Key":"RES_Canvas_ConfirmationMessage","Languages":[{"Name":"en-us","Value":"Are you sure you want to execute this action?"},{"Name":"el","Value":""}]},{"Type":"S","Key":"RES_TOOLTIP_Canvas","Languages":[{"Name":"en-us","Value":""},{"Name":"el","Value":""}]},{"Type":"S","Key":"RES_Layout1_ConfirmationMessage","Languages":[{"Name":"en-us","Value":"Are you sure you want to execute this action?"},{"Name":"el","Value":""}]},{"Type":"S","Key":"RES_TOOLTIP_Layout1","Languages":[{"Name":"en-us"},{"Name":"el","Value":""}]},{"Type":"S","Key":"RES_RowControl2_ConfirmationMessage","Languages":[{"Name":"en-us","Value":"Are you sure you want to execute this action?"},{"Name":"el","Value":""}]},{"Type":"S","Key":"RES_TOOLTIP_RowControl2","Languages":[{"Name":