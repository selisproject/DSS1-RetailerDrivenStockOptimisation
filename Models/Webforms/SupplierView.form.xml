Supplier.Id);\n\tforeach Domain.OrderForecastDetails oFDetail in Model.SelectedOrderFDetails {\n\t\tif oFDetail.Notification != null && oFDetail.Notification.Message == \"Low Stock\"  {\n\t\t\toFDetail.SupplierCanDeliver = false;\n\t\t}\n\t\telse {\n\t\t\toFDetail.SupplierCanDeliver = true;\n\t\t}\n\t\toFDetail.Status = \"Answered\";\n\t}\n\t\n\tModel.AlertsCount = Model.SelectedOrderFDetails.Count(a=> a.Notification != null && a.Notification.Message == \"Low Stock\" \n\t\t\t\t\t&& a.Notification.Item.Supplier.Id == Model.Supplier.Id);\n\n\n\tif Model.AlertsCount == 0 {\n\t\tsupParameter.SupplierCanDeliverWholeForecast = true;\n\t}\n    supParameter.Save();\n\tDebugLib.Logger.WriteInfoLine(\"Supplier shares excess stock.\");\n\t\n\t\n\t// changed because there was an error\n\tvar updatedNotifications = Domain.Notification.Find(a=> a.OrderForecastDetails != null); \n\t\n\t\n\t// Send inApp Notification for each alert\n\tvar supplierAlerts = updatedNotifications.Where(a=> a.Message == \"Low Stock\" \n\t\t\t\t&& a.Item.Supplier.Id == Model.Supplier.Id \n\t\t\t\t&& Model.SelectedOrderFDetails.Contains(a.OrderForecastDetails));\n\t\n\tforeach Domain.Notification notif in supplierAlerts {\n\t\t\n\t\tDomain.AppNotification.CreateAppNotification(notif.Item.SKU, notif.Diff, \"Sonae\", notif.OrderForecastDetails);\n\t}\t\t\t\t\n\t\n    \n    Model.SelectedForecast.Save();\n    Model.SelectedForecast = null;\n    FormControls.lstOrderForecasts.Refresh();\n    FormControls.listOrderForecastDetails.Refresh();\n    FormControls.chartOrderForecast.Refresh();\n    FormControls.chartStockLevels.Refresh();\n\tFormControls.ModalExcessStock.Hide();\n\tModel.SupplierReplied = true;\n\t\n\tShowMessage(\"Your reply was sent successfully.\");\n}","Name":"ReplyToOrderForecast","Permissions":[],"AllowAnonymous":false,"AllowAllAuthenticatedUsers":true,"AccessLog":false,"IsEntryPoint":false,"IsDefault":false,"CausesValidation":true,"DontPostViewModel":false},{"Code":"function void SendForecast