  "Actions": [
    {
      "$type": "CLMS.AppDev.DomainModel.Models.FormControllerAction, CLMS.AppDev.DomainModel",
      "Code": "function void Render()\r\n{\r\n    Model.Title = LocalResources.RES_PAGETITLE_Render;\r\n    \r\n    Model.StackTrace = AppLib.Application.GetLastError().StackTrace;\r\n    Model.ErrorMessage = AppLib.Application.GetLastError().Message;\r\n    Model.FriendlyErrorMessage = AppLib.Application.GetLastError().GetFriendlyMessage();\r\n\r\n}",
      "Name": "Render",
      "Permissions": [],
      "AllowAnonymous": true,
      "AllowAllAuthenticatedUsers": true,
      "AccessLog": false,
      "IsEntryPoint": true,
      "CausesValidation": false,
      "EventName": ""
    },
    {
      "$type": "CLMS.AppDev.DomainModel.Models.FormControllerAction, CLMS.AppDev.DomainModel",
      "Code": "function void SendErrorToAdministrator()\r\n{\r\n    string developerEmail = AppLib.Application.GetConfigurationKey(\"AdministratorEmail\");\r\n    if(string.IsNullOrWhiteSpace(developerEmail))\r\n    {\r\n        ShowMessage(LocalResources.RES_CUSTOM_Invalid_AdminEmail_Message, AppLib.MessageType.Error);\r\n        return;\r\n    }\r\n\r\n    Model.StackTrace = AppLib.Application.GetLastError().StackTrace;\r\n    Model.ErrorMessage = AppLib.Application.GetLastError().Message;\r\n    Model.FriendlyErrorMessage = AppLib.Application.GetLastError().GetFriendlyMessage();\r\n\r\n\tCommonLib.EmailMessage mail;\r\n\t\r\n\tCollection[string] recipients;\r\n\trecipients.Add(developerEmail);\r\n\t\r\n\tmail.To = recipients;\r\n\tmail.Subject = AppLib.Application.Name + \" threw an Exception\";\r\n\tmail.Body = \"<b>Error Message:</b> \" + Model.ErrorMessage + \"<br /><br />\";\r\n\tmail.Body = mail.Body + \"<b>Friendly Error Message:</b> <span style='white-space: pre-line;'>\" + Model.FriendlyErrorMessage + \"</span><br /><br />\";\r\n\tmail.Body = mail.Body + \"<b>StackTrace:</b> \" + Model.StackTrace + \"<br />\";\r\n\tif(!string.IsNullOrWhiteSpace(Model.AdditionalErrorInformation))\r\n\t{\r\n\t    mail.Body = mail.Body + \"<br /><b>Additional Information:</b> \" + Model.AdditionalErrorInformation;\r\n\t}\r\n\t\t\r\n\tCommonLib.Utilities.SendEmail(mail);\r\n\tShowMessage(LocalResources.RES_CUSTOM_Successful_EMail_Message);\r\n}",
      "Name": "SendErrorToAdministrator",
      "Permissions": [],
      "AllowAnonymous": true,
      "AllowAllAuthenticatedUsers": true,
      "AccessLog": false,
      "IsEntryPoint": false,
      "CausesValidation": false,
      "EventName": ""
    }
  ],
