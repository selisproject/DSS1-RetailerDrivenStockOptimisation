 <div id="lstOrderForecasts_CompositeFiltersModal" class="modal fade data-list-filters-modal" role="dialog" jb-composite-filters ng-model-options="{ updateOn: 'default' }">
      <div class="modal-dialog" style="width: 70vw">
        <div class="modal-content">
            <div class="modal-header">                    
                <h4 class="modal-title">{{$resources.compositeFilters}}</h4>
            </div>
            <div class="modal-body">
                <table class="table">
                    <thead>
                        <tr>
                            <th>{{$resources.column}}</th>
                            <th>{{$resources.filterType}}</th>
                            <th>{{$resources.criteria}}</th>
                            <th>{{$resources.rowOperator}}</th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="$filter in $filters track by $index">
                            <td>
                                <select class="form-control" ng-model="$filter.column"
                                        ng-options="column as column.caption for column in $columns track by column.name"></select>
                            </td>
                            <td>
                                <select class="form-control" ng-model="$filter.operator"
                                        ng-options="item.value as item.caption for item in $operators | filter: $findCompatibleOperators($filter)"></select>
                            </td>
                            <td>
                                <!-- Text-->
                                <input ng-hide="$filter.operator == 9 || $filter.operator == 10 || $filter.column.dataType == 'bool' || $filter.column.dataType == 'DateTime'"
                                        class="form-control"
                                        type="text"
                                        ng-model="$filter.value" />

                                <!-- Date -->
                                <input jb-quick-date-time-picker 
                                        formatting="DD/MM/YYYY"
                                        ng-show="$filter.operator != 9 && $filter.operator != 10 && $filter.column.dataType == 'DateTime'"
                                        class="form-control"
                                        type="text"
                                        ng-model="$filter.value" />

                                <!-- True / False -->
                                <select class="form-control"
                                        ng-model="$filter.value"
                                        ng-show="$filter.operator != 9 && $filter.operator != 10 && $filter.column.dataType == 'bool'">
                                    <option value="true">{{$resources.valueTrue}}</option>
                                    <option value="false">{{$resources.valueFalse}}</option>
                                </select>

                                <!-- Range 'And' -->
                                <h5 ng-show="$filter.operator == 8" 
                                    style="text-align: center">{{$resources.and}}</h5>  

                                <!-- Raw Range -->
                                <input ng-show="$filter.operator == 8 && $filter.column.dataType != 'DateTime'"
                                        class="form-control"
                                        type="text"
                                        ng-model="$filter.secondValue" />    
                                
                                <!-- Date Range -->
                                <input jb-quick-date-time-picker 
                                        formatting="DD/MM/YYYY"
                                        ng-show="$filter.operator == 8 && $filter.column.dataType == 'DateTime'"
                                        class="form-control"
                                        type="text"
                                        ng-model="$filter.secondValue" />
                            </td>
                            <td>
                                <select class="form-control" ng-model="$filter.rowOperator"
                                        ng-options="item.value as item.caption for item in $rowOperators"></select>
                            </td>
                            <td>
                                <button class="btn btn-danger pull-right btn-block"
                                        ng-click="$removeFilter($filter)">
                                    <b>&times;</b>
                                </button>                                    
                            </td>
                        </tr>
                    </tbody>
                    <tfoot>
                        <tr>
                            <td colspan="5">
                                <div ng-show="$hasErrors" class="alert alert-danger">
                                    {{$resources.pleaseFillInAllFields}}
                                </div>
                            </td>
                        </tr>
                        <tr>
                            <td colspan="1">                               
                                <button class="btn btn-success"
                                        ng-click="$addFilter()">
                                    <b>+</b>
                                    {{$resources.add}}
                                </button>                               
                            </td>
                            <td colspan="2"></td>
                            <td colspan="2">
                                <button class="pull-right btn btn-danger"
                                        ng-click="$filters = []; $hasErrors = false">
                                    <b>&times;</b>
                                   {{$resources.clearAll}}
                                </button>
                            </td>
                        </tr>                            
                    </tfoot>
                </table>
            </div>
            <div class="modal-footer">
                <button ng-click="$commit()" class="btn btn-primary">{{$resources.ok}}</button>
                <button ng-click="$cancel()" class="btn btn-default">{{$resources.cancel}}</button>
            </div>
        </div>
    </div>
</div>

</div>
</div>
</div>
</div>
